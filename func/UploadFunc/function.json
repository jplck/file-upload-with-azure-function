{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "path": "uploads/{rand-guid}",
      "connection": "outputblobsettings",
      "direction": "out"
    }
  ]
}
